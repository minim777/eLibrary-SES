<!DOCTYPE html>
<html lang="en-AU">
    <%- include('./partials/head.ejs') %>
    <body>
        <div class="tint">
            <div class="pagecontainer">
                <%- include('./partials/nav.ejs') %>
                <main class="maincontent">
                    <!--Search bar for user table-->
                    <div class="searchtable">

                    </div>
                    <!--Table of existing users-->
                    <div class="tablelist">
                        <table>
                            <thead>
                                <tr>
                                    <th class="tableheader" colspan="5">Books currently in eLibrary</th>
                                </tr>
                                <tr>
                                    <th class="tablesubhead" scope="col">Book ID</th>
                                    <th class="tablesubhead" scope="col">Title</th>
                                    <th class="tablesubhead" scope="col">Author(s)</th>
                                    <th class="tablesubhead" scope="col">Type</th>
                                    <th class="tablesubhead" scope="col">More</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% booklist.forEach(book => {%>
                                <tr>
                                    <td><%=book._id%></td>
                                    <td><%=book.Title%></td>
                                    <td><%=book.Author%></td>
                                    <td><%=book.Type%></td>
                                    <td><input type="button" onclick="window.location.href='/book/<%= book._id%>';" value="More"/></td>
                                </tr>
                                <%})%>
                            </tbody>
                        </table>
                    </div>
                </main>
                <%- include('./partials/footer.ejs') %>
            </div>
        </div>
    </body>
</html>